{
  "manifest_version": 2,
  "name": "chatGPT-Conversation-Exporter",
  "version": "1.0",
  "description": "Chat exporter!",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
